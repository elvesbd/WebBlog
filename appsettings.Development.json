{
    "Env": "dev",
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost,1433;Database=<database_name>;User ID=<user>;Password=<password>;Encrypt=false"
    },
    "JwtKey": "jwt_key",
    "ApiKeyName": "api_key_name",
    "ApiKey": "api_key",
    "SmtpConfiguration": {
        "Host": "smtp.sendgrid.net",
        "Port": "587",
        "UserName": "apikey",
        "Password": "password"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}